{
  "name": "@dnajs/dna",
  "description": "Evolution-based components.",
  "version": "2.7.4",
  "scripts": {
    "postinstall": "which rna || yarn add -W git+https://gitlab.com/chialab/rna-cli.git",
    "start": "echo 'Nothing to start' && exit 1",
    "serve": "echo 'Nothing to serve' && exit 1",
    "build": "rna build",
    "clean": "rm -rf node_modules/ && rna bootstrap",
    "unit": "rna unit",
    "test": "rna unit",
    "test-saucelabs": "rna unit --saucelabs",
    "test-electron": "rna unit --electron",
    "lint": "rna lint",
    "dist": "rna build --production --no-map",
    "docs": "echo 'Documentation builder not set up' && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chialab/dna.git"
  },
  "author": "Chialab <dev@chialab.io> (https://chialab.io)",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/chialab/dna/issues"
  },
  "homepage": "https://dna.chialab.io",
  "devDependencies": {
    "codecov.io": "^0.1.6",
    "eslint": "^4.12.0",
    "eslint-plugin-mocha": "^4.11.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
